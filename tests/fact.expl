// Input a number and print its factorial

function fact (x : int) : int {
  let c : int = 1
  let z : int = 1
  while 0 < x {
    z := z * x
    x := x - c
  }
  return z
}

function exec : int {
  let x : int = 0
  let y : int = 0
  let r : int = 0
  input x
  y := fact(x)
  output y
  return r
}
